{
  "include": [
    "src/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true,
    "moduleResolution": "node",
  },
  "lib": [
    "WebWorker"
  ],
}